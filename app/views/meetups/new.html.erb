<div class="row">
	<div class="col s8">
		<h5>Create a New Meetup</h5>
		<%# <p><i>Select a Date and Location</i></p> %>
		<br />
		<%= form_for [@group,@meetup] do |f| %>
			<div class="input-field">
				<%= f.text_field :date, class: 'datepicker' %>
				<%= f.label :date, 'Select a Date' %>
			</div>
			<%= f.submit 'View the best locations' %>
		<% end %>
	</div>
	<div class="col s4"></div>
</div>
